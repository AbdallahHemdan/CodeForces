//freopen("input.txt","r",stdin);
//freopen("output.txt","w",stdout);
using namespace std ;
int main(){
    int n ; 
    cin >> n ; 
    string s ; 
    cin >> s;  
    int len = s.size() ; 
    int A_counter = 0 ; 
    int D_counter = 0 ; 
    for(int i=0 ;i<len ; i++)
        if(s[i]=='A')
            A_counter++ ; 
        else
            D_counter++ ; 
    if(A_counter>D_counter)
        cout << "Anton" << endl ;
    else if(A_counter<D_counter)
        cout << "Danik" << endl ;
    else
        cout << "Friendship" << endl ;
    
}
